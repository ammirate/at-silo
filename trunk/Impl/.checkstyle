<?xml version="1.0" encoding="UTF-8"?>

<fileset-config file-format-version="1.2.0" simple-config="false" sync-formatter="false">
  <local-check-config name="Atsilo coding style" location="checkstyle.xml" type="project" description="Stile di codifica per il progetto Atsilo">
    <additional-data name="protect-config-file" value="false"/>
  </local-check-config>
  <fileset name="all" enabled="true" check-config-name="Atsilo coding style" local="true">
    <file-match-pattern match-pattern=".java$" include-pattern="true"/>
  </fileset>
  <filter name="UnOpenedFiles" enabled="true"/>
  <filter name="FilesInSync" enabled="true"/>
</fileset-config>
